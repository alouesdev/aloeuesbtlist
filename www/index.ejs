<%- include('parts/head', {bot, user, path}) %>


<section class="bg-vcodes jumbotron">
    <div class="container px-3 mt-3 mb-5">
      <div class="mb-3 mx-auto has-text-centered">
        <img draggable="false" style="z-index: -1!important" src="https://cdn.discordapp.com/attachments/827932568598741032/848853364254703656/gliotbotlistwhite.png" width="220" alt="Discord Bots | Discord Bot List"><br><br><br>
        <h1 class="has-text-white title" style="margin-top: -2.3%; font-weight: 800;">Gliot Botlist</h1>
        <p class="content has-text-white" style="opacity: 0.6; margin-top: -20px;">
          Discord botunuzu genişletmek ve geliştirmek istemez misiniz? İşte Gliot Botlist sizin için burada!</p>
          <form action="/search">
              <input style="background-color: #2c2f33;" id="searchbar"  name="q" onkeyup="search_bot()" type="text"
              name="search" autocomplete="off" onfocus="if (!window.__cfRLUnblockHandlers) return false; $('.searchArea').show()" id="search" style="font-family: Arial, FontAwesome; z-index: 999999999; width: 470px;" class="z mainInput searchIcon isMobile" type="text" placeholder="Bot Ara..." data-cf-modified-510f8a7e40ac29a46d013099-="">
              <input type="hidden" name="page" value="1">
            </form>
        <div class="home-tags">
              <% config.tags.slice(0, 3).map(a => {%>
                  <a style="margin: 5px;" title="Gliot Botlist" href="/bots?tag=<%=a %>" class="btn btn-tag">
                      <%= a %>
                      </a>
                <% }) %>
                <a style="margin: 5px;" title="Gliot Botlist" href="/bots?tag=All" class="btn btn-tag">
                  Other Tags
              </a>
        </div>
      </div>
    </div>
</div>
</section>



<div class="container">
  <br><br>
<div class="column">
    <p class="has-text-white title is-large"><i class="fa fa-badge-check has-text-warning"></i>&nbsp;Certified Bots</p>
    <p class="content has-text-white" style="margin-top: -20px;">They are ranked according to the votes they received.</p>
    <div class="columns is-multiline is-desktop is-variable is-7 pt-5">
        <% botdata.filter(a => a.certificate == "Certified").sort((a, b) => b.votes - a.votes).slice(0, 6).map(a => {%>
      <div class="col-md-6 col-lg-4 pb-3">
        <div data-aos="fade-left">
        <div class="card vcodes-card-custom bg-vcodes border-white border-0">
          <div class="vcodes-card-custom-img" onerror="backImgError(this);" style="background-image: url(<%= a.backURL || 'https://i.hizliresim.com/2sdunq.png' %>);"></div>
          <div class="vcodes-card-custom-avatar">
            <img class="img-fluid" style="border-radius:50%;" onerror="imgError(this);" src="<%=a.avatar%>" alt="Avatar" />
          </div>
          <div class="card-body">
           <div class="is-pulled-right mt-vcodes mt-1">
            <span data-tooltip="Toplam Oy" data-tooltip-location="left" class="badge badge-stats mb-vcodes mt-1 w-100"><i class="fad fa-chevron-up"></i>&nbsp;<%= a.votes || "0" %></span>
         
              </div>
            <h4 translate="no" class="card-title"><%= a.username %>
            <% if(a.certificate === "Certified") { %>
              <span data-tooltip="Verified Bot" data-tooltip-location="top"><i class="fad fa-badge-check has-text-success"></i></span>
            <% } %>
            </h4>
            <p translate="no" class="card-text vCodesbotDesc"><%= a.shortDesc %></p>
          </div>
          <div class="card-footer">
            <a href="/bot/<%=a.botID%>" class="btn btn-tag btn-bot">İncele</a>
            <a href="https://discord.com/oauth2/authorize?client_id=<%=a.botID%>&scope=bot&permissions=8" class="btn btn-tag btn-bot">Davet Et</a>
          </div>
        </div>
      </div>
      </div>
            <% }) %>
             </div>
</div>
  <% if(botdata.filter(a => a.certificate == "Certified").length > 6) { %>
  <center>
    <a href="/bots/certified" style="align-items: center; width: 55%;" class="btn btn-back has-text-white"><i class="fad fa-badge-check has-text-success"></i>&nbsp;Show more certified bots</a>
  </center>
  <% } %>
